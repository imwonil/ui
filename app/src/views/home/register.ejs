    <div class="hidden">
      <textarea></textarea>
    </div>
   <div class="flex items-center justify-center flex-col [&>div]:m-1 [&>div>button]:bg-slate-600 [&>div>button]:border [&>div>button]:text-white [&>div>button]:rounded-md [&>div>button]:text-center">

     <!--요 바로 위에 있는 거는 맨 윗줄과 아랫줄만 통제함..-->

     <div class="flex items-center justify-center gap-2 [&>button]:w-9 [&>button]:p-1">
      <button class="btn">1</button>
      <button class="btn">2</button>
      <button class="btn">3</button>
      <button class="btn">4</button>
      <button class="btn">5</button>
      <button class="btn">6</button>
      <button class="btn">7</button>
      <button class="btn">8</button>
      <button class="btn">9</button>
      <button class="btn">0</button>
      <button class="delete"><i class="fa-solid fa-arrow-right rotate-180"></i></button>
   </div>


      <div class="en-keyboard [&>div]:flex [&>div]:items-center [&>div]:justify-center 
        [&>div]:gap-2 [&>div]:my-2">
         <div class="[&>button]:bg-slate-600 [&>button]:border [&>button]:p-1 [&>button]:w-9 [&>button]:text-white [&>button]:rounded-md [&>button]:text-center">
          <button class="btn">q</button>
          <button class="btn">w</button>
          <button class="btn">e</button>
          <button class="btn">r</button>
          <button class="btn">t</button>
          <button class="btn">y</button>
          <button class="btn">u</button>
          <button class="btn">i</button>
          <button class="btn">o</button>
          <button class="btn">p</button>
        </div>

        <div class="[&>button]:bg-slate-600 [&>button]:border [&>button]:p-1 [&>button]:w-9 [&>button]:text-white [&>button]:rounded-md [&>button]:text-center">
