<body class="h-auto flex justify-center items-center flex-col">

  <!-- 회원 가입 -->
        <!-- Modal -->
        <div class=" flex items-center justify-center z-50 regi-modal">
            <div class="bg-white rounded-lg p-6 w-96 max-w-full shadow-lg transform transition-all duration-300">
                <!-- Modal Header -->
                <div class="flex justify-between items-center border-b-2 border-gray-200 pb-4">
                    <h2 class="text-xl font-semibold">회원가입</h2>
                  
                </div>

                <!-- Modal Content -->
                <div class="mt-6 space-y-4">
                  <span>
                    <input type="text" id="verification" class="outline-none w-full text-center p-1 py-2 bg-gray-100 focus:bg-white rounded-lg my-2" placeholder="인증번호">
                

                    <input type="text" id="name" class="outline-none w-full text-center p-1 py-2 bg-gray-100 focus:bg-white rounded-lg my-2" placeholder="이름">

                    <input type="text" id="id" class="outline-none w-full text-center p-1 py-2 bg-gray-100 focus:bg-white rounded-lg my-2" placeholder="아이디">
                    <input type="text" id="psword" class="outline-none w-full text-center p-1 py-2 bg-gray-100 focus:bg-white rounded-lg my-2" placeholder="비밀번호">
                    <input type="text" id="confirm-psword" class="outline-none w-full text-center p-1 py-2 bg-gray-100 focus:bg-white rounded-lg my-2" placeholder="비밀번호 확인">

                    <p><button class="bg-gray-900 text-gray-100 px-4 py-2 rounded-lg hover:bg-black transition duration-300 block my-2 w-full">
                     가입하기
                      </button></p>
                    </span>
                </div>
            </div>
        </div>




  <!-- 가상 키보드 시작-->

  <!--keyboard-container 클래스명 수정-->
