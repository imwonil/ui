  <script>
    $(".tabs li").click(function(){
      $(this).addClass("actived")
      $(this).siblings().removeClass("actived")

      var tab = $(this).attr("data-alt")
      $(".wrapper div").removeClass("actived-form")
      $("#" + tab).addClass("actived-form")
    })

  </script>


